    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Tool 6.0 graph resolver",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/src/Ionide.ProjInfo.Tool/bin/Debug/net6.0/Ionide.ProjInfo.Tool.dll",
            "args": [
                "--project",
                "<your project>",
                "--graph"
            ],
            "cwd": "${workspaceFolder}/src/Ionide.ProjInfo.Tool",
            "stopAtEntry": false,
            "console": "internalConsole",
            "justMyCode": true,
        },
        {
            "name": "Launch Tool 6.0 normal resolver",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/src/Ionide.ProjInfo.Tool/bin/Debug/net6.0/Ionide.ProjInfo.Tool.dll",
            "args": [
                "--project",
                "<your project>",
            ],
            "cwd": "${workspaceFolder}/src/Ionide.ProjInfo.Tool",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "justMyCode": false,
            "symbolOptions": {
                "searchPaths": [],
                "searchMicrosoftSymbolServer": false,
                "searchNuGetOrgSymbolServer": true
            }
        },
        {
            "name": "Ionide.ProjInfo.Tests",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/test/Ionide.ProjInfo.Tests/bin/Debug/net6.0/Ionide.ProjInfo.Tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}